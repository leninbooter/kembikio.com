.message h1
	text-align center
	padding 10px
	
.paypal
	width 50%
	float right
	text-align center
	vertical-align middle
	
.paypal input
	width auto

.paypal h2
	margin-top 0
	
.paypal img
	vertical-align middle
	
.redes_aceptadas
		padding 20px
	
.tpv
	width 50%
	float left
	text-align center
	
#compra_lineatemporal
	width 100%
	text-align center
	padding 0
	
#compra_lineatemporal li
	display inline-block
	width 33.3%
	height 10px
	background #ccc	
	text-align right
	margin-top 40px
	margin 40px 0px 20px 0px
	vertical-align top

#compra_lineatemporal li:first-child
#compra_lineatemporal li:last-child
	border-radius 10px 0px 0px 10px
	
#compra_lineatemporal div
	text-align: center;
	position: relative;
	display: block;
	margin-top: -50px;
	width 100%
	right -50%
	
#compra_lineatemporal li h1
	font-size 20px
	white-space nowrap
	visibility hidden
	
#compra_lineatemporal li a
	background #fff
	position relative
	border 2px solid #ccc
	border-radius 50%
	padding 2px 8px 5px
	bottom 10px
	margin-right -5px
	height 20px
	width 10px
	display inline-block
	
#contenido
	width 70%
	margin 0 auto