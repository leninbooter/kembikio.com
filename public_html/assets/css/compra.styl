table
	width 100%

table th
table td
	text-align left
	border 1px solid #dedede
	
table th
	background #666666
	color #fff
	font-size 1.2em
	font-weight normal
	padding 5px 10px
	
table td
	padding 12px 10px
	font-size 1em

table td:last-child
	font-size 1.5em
	font-weight bold

.actual
.completado
	background #33A1D9 !important

.actual a
.completado a
	border 2px solid #1676a7 !important
	
.actual h1
.completado h1
	visibility visible !important

.base_controles
	position relative
	
.base_controles button
	position absolute
	border-radius 0%
	padding 5px 13px
	right 0px
	margin 0
	border 0px solid #ff6d00
	background #fa8935
	color #fff
	font-size 

.boton_descarga
	margin 0 auto
	width 100%
	text-align center
	
.base_controles button:hover
.button:hover
	background #ff6d00
	
.button
	border-radius 0%
	padding 5px 13px
	margin 0
	border 0px solid #ff6d00
	background #fa8935
	color #fff
	font-size 
	
.completado a
	background #49c0fd

.form_pedido
	margin 10px 0 50px

.form_pedido fieldset
	border 1px solid #DEDEDE
	margin 0
	
.form_pedido .form
	width 60%

.form_pedido .form div
	margin 0 0 10px 0
	
.form_pedido .form div > label
	display block
	font-size 14px
	margin 0 0 5px 0
	
.form_pedido .form #nombre
.form_pedido .form #apellido
	width 20% /*111px*/
	padding 5px 0px 5px 5px
	
.form_pedido .form #cp
	width 50px
	
.form_pedido .info
	width 40%

