table
	width 100%

table th
table td
	text-align left
	border 1px solid #dedede
	
table th
	background #666666
	color #fff
	font-size 1.2em
	font-weight normal
	padding 5px 10px
	
table td
	padding 12px 10px
	font-size 1em

table td:last-child
	font-size 1.5em
	font-weight bold

.actual
	background #33A1D9 !important

.actual h1
.completado h1
	visibility visible !important

.base_controles
	position relative
	
.base_controles button
	position absolute
	border-radius 0%
	padding 5px 13px
	right 0px
	margin 0
	border 0px solid #ff6d00
	background #fa8935
	color #fff
	font-size 

.base_controles button:hover
	background #ff6d00
	
.completado a
	background #49c0fd

.form_pedido
	margin 10px 0 50px

.form_pedido fieldset
	border 1px solid #DEDEDE
	margin 0
	
.form_pedido .form
	width 60%

.form_pedido .form div
	margin 0 0 10px 0
	
.form_pedido .form div > label
	display block
	font-size 14px
	margin 0 0 5px 0
	
.form_pedido .form #nombre
.form_pedido .form #apellido
	width 20% /*111px*/
	padding 5px 0px 5px 5px
	
.form_pedido .form #cp
	width 50px
	
.form_pedido .info
	width 40%
	
#compra_lineatemporal
	width 100%
	text-align center
	padding 0
	
#compra_lineatemporal li
	display inline-block
	width 33.3%
	height 10px
	background #ccc	
	text-align right
	margin-top 40px
	margin 40px 0px 20px 0px
	vertical-align top

#compra_lineatemporal li:first-child
#compra_lineatemporal li:last-child
	border-radius 10px 0px 0px 10px
	
#compra_lineatemporal div
	text-align: center;
	position: relative;
	display: block;
	margin-top: -50px;
	width 100%
	right -50%
	
#compra_lineatemporal li h1
	font-size 20px
	white-space nowrap
	visibility hidden
	
#compra_lineatemporal li a
	background #fff
	position relative
	border 2px solid #1676a7
	border-radius 50%
	padding 2px 8px 5px
	bottom 10px
	margin-right -5px
	height 20px
	width 10px
	display inline-block
	
#contenido
	width 70%
	margin 0 auto